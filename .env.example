APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
APP_TIMEZONE=Asia/Manila

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# SMS delivery options: log, infobip
SMS_DRIVER=log
# For local testing without SMS, set SMS_DRIVER=log and optionally SMS_LOG_CHANNEL=stack
# To use Infobip as the SMS provider, set SMS_DRIVER=infobip and provide the following
# values. Do NOT commit real secrets to version control; keep them in your local .env.
#
# INFOBIP_API_KEY  - Your Infobip API key (create/manage in Infobip Portal)
# INFOBIP_SENDER   - The registered sender (shortcode or alphanumeric sender)
# INFOBIP_BASE_URL - Optional Infobip base URL (default below)
#
# Example (local .env):
# SMS_DRIVER=infobip
# INFOBIP_API_KEY=sk_live_XXXXXXXXXXXXXXXXXXXXXXXXXXXX
# INFOBIP_SENDER=HMBBank

INFOBIP_API_KEY=
INFOBIP_SENDER=
INFOBIP_BASE_URL=https://api.infobip.com

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
